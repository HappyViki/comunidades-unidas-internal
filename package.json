{
  "name": "comunidades-unidas-internal",
  "description": "the code for comunidades unidas internal website",
  "repository": "git@github.com:JustUtahCoders/comunidades-unidas-internal.git",
  "author": "Joel Denning",
  "license": "MIT",
  "scripts": {
    "fix-everything": "docker-compose down && docker-compose pull && yarn install",
    "start": "node backend/server.js",
    "develop": "concurrently -n w: yarn:develop-*",
    "develop-frontend": "PUBLIC_PATH='http://localhost:9018/' webpack-dev-server --open",
    "develop-backend": "RUNNING_LOCALLY='true' nodemon backend/server.js --watch backend",
    "develop-database": "docker-compose up",
    "build": "rimraf static && concurrently -n w: yarn:build-*",
    "build-frontend": "webpack --config webpack.config.js",
    "nuke-database": "docker-compose down -v",
    "db-migrate-prod": "db-migrate up --env prod",
    "typescript": "tsc"
  },
  "husky": {
    "pre-commit": "yarn typescript"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "kremling": "^1.2.4",
    "mysql": "^2.16.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@zeit/ncc": "^0.15.2",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
